
config system
	option hostname 'BoluWrt'
	option zonename 'Asia/Jakarta'
	option timezone 'WIB-7'
	option conloglevel '8'
	option cronloglevel '8'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option enabled '1'

config led 'led_wan'
	option name 'wan'
	option sysfs 'bolt-bl201:blue:wan'
	option default '0'
	option trigger 'netdev'
	option dev 'eth0.2'
	option mode 'tx rx'

config led 'led_lan'
	option name 'lan'
	option sysfs 'bolt-bl201:blue:wps'
	option default '0'
	option trigger 'netdev'
	option dev 'eth0.1'
	option mode 'tx rx'

config led 'led_wifi_led'
	option name 'wifi'
	option sysfs 'bolt-bl201:blue:wlan'
	option default '0'
	option trigger 'netdev'
	option dev 'wlan0'
	option mode 'tx rx'

config led 'led_wifi_led'
	option name 'wifi'
	option sysfs 'bolt-bl201:blue:wlan'
	option default '0'
	option trigger 'netdev'
	option dev 'wlan1'
	option mode 'tx rx'


config led 'led_power'
	option name 'power'
	option sysfs 'bolt-bl201:blue:power'
	option default '0'
	option trigger 'default-on'

config led
	option default '0'
	option name 'powerRed'
	option sysfs 'bolt-bl201:red:power'
	option trigger 'none'

